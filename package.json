{
  "name": "api",
  "private": true,
  "version": "0.0.1",
  "description": "backend",
  "scripts": {
    "start": "cross-env NODE_CONFIG_DIR=./src/config cross-env TESTING=true nodemon --config ./nodemon.json --exec ts-node ./src/bin/www.ts",
    "build": "gulp build",
    "build-with-docs": "gulp build-with-docs",
    "lint-noFix": "gulp lint-noFix",
    "lint": "npm run prettier && npm run tslint",
    "test": "npm run build && npm run reset-test-db && npm run start-test-system",
    "copy-assets": "gulp copy-assets && gulp replace",
    "clean-build": "gulp clean-build",
    "compile": "tsc",
    "prettier": "prettier '{src/**/*.*,typings/**/*.*,tests/**/*.*}' --write",
    "tslint": "tslint -c tslint.json -p tsconfig.json -t stylish --fix '{src,tests}/**/*.ts'"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "glob": "^7.1.3",
    "got": "^9.3.0",
    "issue-maker": "^1.0.1",
    "joi": "^14.3.1",
    "lme": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "jszip": "2",
    "mongoose": "^5.5.11",
    "morgan": "^1.9.0",
    "read-package-json": "^2.0.13",
    "serialize-error": "^2.1.0",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.15",
    "swagger-jsdoc": "^1.9.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/config": "^0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/del": "^3.0.1",
    "@types/express": "^4.16.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-rename": "^0.0.33",
    "@types/gulp-replace": "^0.0.31",
    "@types/html-minifier": "^3.5.2",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jszip": "^3.1.4",
    "@types/mongoose": "^5.5.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.2",
    "@types/run-sequence": "^0.0.30",
    "@types/serialize-error": "^2.1.0",
    "@types/serve-favicon": "^2.2.30",
    "del": "^3.0.0",
    "git-last-commit": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-exec": "^3.0.2",
    "gulp-markdown": "^3.0.0",
    "gulp-prettier-plugin": "^1.3.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "html-minifier": "^3.5.21",
    "markdown-table": "^1.1.2",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3",
    "run-sequence": "^2.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.4.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.spec.{ts,js}"
    ],
    "testEnvironment": "node"
  }
}